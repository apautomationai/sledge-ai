{
  "name": "api",
  "version": "1.0.0",
  "description": "Express.js API with TypeScript and Drizzle ORM",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "drizzle": "drizzle-kit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "express",
    "typescript",
    "drizzle-orm",
    "api"
  ],
  "author": "",
  "license": "ISC",
